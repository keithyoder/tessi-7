    "@hotwired/turbo-rails": "^7.3.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "chart.js": "^4.4.1",
    "chartkick": "^5.0.1",
    "jquery": "^3.7.1"
  },
  "devDependencies": {
    "esbuild": "^0.27.2",
    "sass": "^1.70.0"
  }
}